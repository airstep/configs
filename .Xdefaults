URxvt.title:            Terminal
URxvt.buffered:         true
URxvt.borderLess:       false
URxvt.background:       black
URxvt.foreground:       white
URxvt.cursorColor:      green
URxvt.underlineColor:   yellow
URxvt.font:             xft:Monospace:pixelsize=12:antialias=true
URxvt.boldFont:         xft:Monospace:bold:pixelsize=12:antialias=true
URxvt.italicFont:       xft:Monospace:italic:pixelsize=12:antialias=true
URxvt.perl-ext-common:  default,tabbed,matcher,clipboard,url-select
URxvt.keysym.M-u:  		perl:url-select:select_next
URxvt.keysym.Control-Insert: perl:clipboard:copy
URxvt.keysym.Shift-Insert: perl:clipboard:paste

URxvt.perl-ext: default,url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: firefox
URxvt.url-select.underline: false

URxvt.matcher.button:   1
URxvt.scrollBar:	    false
URxvt.tabbed.tabbar-fg: 8 
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg:    3 
URxvt.tabbed.tab-bg:    0
urxvt.depth:			32
urxvt.background:		rgba:0000/0000/0000/dddd

