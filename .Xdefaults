!==============================================================================
! XTerm 
!==============================================================================
!Кириллица
XTerm*utf8Title: true
XTerm*faceName: Droid Sans Mono
XTerm*faceSize: 10
xterm*bolfFont: 
xterm*scrollBar:        false
xterm*geometry:         100x30
xterm*eightBitInput:    false
xterm*metaSendsEscape:  true
xterm*colorBDMode:      true
xterm*boldMode:         on
xterm*alwaysBoldMode:   false
xterm*dynamicColors:    on
xterm*colorMode:        on
xterm*saveLines:        10000
xterm*visualBell:       false

xterm.perl-ext-common:  default,tabbed,matcher,clipboard,url-select
xterm.keysym.M-u:  		perl:url-select:select_next
xterm.keysym.Control-Insert: perl:clipboard:copy
xterm.keysym.Shift-Insert: perl:clipboard:paste

